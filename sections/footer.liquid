  <hr>
  <div class="daybreak-footer__content-bottom page-width">
    <div class="footer__copyright caption">
      <small class="copyright__content">&copy; {{ 'now' | date: '%Y' }}, {{ shop.name }}</small>
      <ul class="policies list-unstyled day-flex-row footer-policy-links">
        <li>
          <small class="copyright__content"><a href="/pages/accessibility" class="text-color-primary">Accessibility</a></small>
        </li>
        {% for policy in shop.policies %}
          {% if policy != blank %}
            <li>
              <small class="copyright__content">
                <a href="{{ policy.url }}" class="text-color-primary">{{ policy.title | escape }}</a>
              </small>
            </li>
          {% endif %}
        {% endfor %}
      </ul>
    </div>
  </div>
</footer>

{% schema %}
{
  "name": "Footer Daybreak",
  "enabled_on": {
    "templates": ["*"],
    "groups": ["footer"]
  },
  "blocks": [
    {
      "type": "@app"
    },
    {
      "type": "link_list",
      "name": "Menu",
      "settings": [
        {
          "type": "inline_richtext",
          "id": "heading",
          "default": "Heading",
          "label": "Quick links"
        },
        {
          "type": "link_list",
          "id": "menu",
          "default": "footer",
          "label": "Menu",
          "info": "Displays only top-level menu items"
        }
      ]
    }
  ],
  "settings": [
    {
      "type": "checkbox",
      "id": "newsletter_enable",
      "default": true,
      "label": "Show newsletter signup"
    },
    {
      "type": "inline_richtext",
      "id": "newsletter_heading",
      "default": "Subscribe to our emails",
      "label": "Newsletter Heading"
    }
  ],
  "default": {
    "blocks": [
      {
        "type": "link_list"
      }
    ]
  }
}
{% endschema %}
